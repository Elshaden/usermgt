{
    "name": "elshaden/livewire-usermgt",
    "description": "Livewire User Management",
    "keywords": [
        "elshaden",
        "laravel",
        "livewire-usermgt"
    ],
    "homepage": "https://github.com/elshaden/livewire-usermgt",
    "license": "MIT",
    "authors": [
        {
            "name": "Salah Elabbar",
            "email": "salah@elshaden.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37",
        "livewire/livewire": "^2.7",
        "prettus/l5-repository": "^2.7",
        "spatie/laravel-package-tools": "^1.9.2",
        "spatie/laravel-permission": "^5.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.10",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.4",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Elshaden\\LivewireUsermgt\\": "src",
            "Elshaden\\LivewireUsermgt\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elshaden\\LivewireUsermgt\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elshaden\\LivewireUsermgt\\LivewireUsermgtServiceProvider"
            ],
            "aliases": {
                "LivewireUsermgt": "Elshaden\\LivewireUsermgt\\LivewireUsermgtFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
